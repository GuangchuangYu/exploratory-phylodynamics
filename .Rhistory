samplecluster <- function(n){
idx <- seq(1,n)
nidx <- c(n,rep(0,n-1))
result <- matrix(0,nrow=n,ncol=n)
result[1,] <- nidx
for(i in 2:(n-1)){
a1 <- sample(x=idx,size=1,replace=FALSE,prob=nidx)
nidx[a1] <- nidx[a1]-1
a2 <- sample(x=idx,size=1,replace=FALSE,prob=nidx)
nidx[a2] <- nidx[a2]-1
nidx[a1+a2] <- nidx[a1+a2]+1
result[i,] <- nidx
}
result[n,n] <- n
result
}
propclustered <- function(r){
rs <- rowSums(r)
(rs-r[,1])/rs
}
